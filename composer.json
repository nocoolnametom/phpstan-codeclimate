{
    "name": "nocoolnametom/phpstan-codeclimate",
    "description": "Code Climate error formatter for PHPStan",
    "license": "MIT",
    "minimum-stability": "stable",
    "extra": {
      "phpstan": {
        "includes": [
          "extension.neon"
        ]
      }
    },
    "require": {
        "php": "~7.1",
        "phpstan/phpstan": "^0.11",
        "nette/utils": "^2.4.5 || ^3.0",
        "symfony/console": "*"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.0"
    },
    "autoload": {
      "psr-4": {
        "NoCoolNameTom\\PHPStan\\ErrorFormatter\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "NoCoolNameTom\\PHPStan\\ErrorFormatter\\": "src/"
      }
    },
    "authors": [
        {
            "name": "Tom Doggett",
            "email": "nocoolnametom@gmail.com"
        }
    ]
}
